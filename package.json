{
  "name": "electaco",
  "version": "0.2.5",
  "description": "Guild Wars 2 Overlay",
  "main": "build/electron/ts_static/electron_main.js",
  "homepage": "./",
  "repository": {
    "type": "git",
    "url": "https://github.com/electaco/overlay.git"
  },
  "watch": {
    "react-build": {
      "patterns": [
        "src",
        "public"
      ],
      "extensions": [
        "js,ts,html,css,scss,tsx"
      ],
      "delay": 500
    },
    "electron-build": {
      "patterns": [
        "ts_static",
        "src/shared"
      ],
      "extensions": [
        "js,ts,html,css,scss"
      ]
    }
  },
  "scripts": {
    "watch": "npm-watch",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "electron .",
    "electron-build": "tsc -p tsconfig-electron.json",
    "build-all": "npm run electron-build && npm run react-build",
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "deploy": "electron-builder --publish always",
    "build": "electron-builder --publish never"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "@types/jest": "^26.0.10",
    "@types/node": "^16.11.6",
    "@types/react": "^16.9.48",
    "@types/react-dom": "^16.9.8",
    "class-transformer": "^0.4.0",
    "cross-fetch": "^3.1.5",
    "electron-updater": "^4.3.9",
    "js-yaml": "^3.13.1",
    "lodash.isequal": "^4.5.0",
    "object-path": "^0.11.8",
    "reflect-metadata": "^0.1.13",
    "three": "^0.130.1",
    "uuid": "^7.0.3",
    "video.js": "^7.16.0",
    "ws": "^8.2.0"
  },
  "author": "Terrasque",
  "license": "ISC",
  "devDependencies": {
    "@types/three": "^0.130.0",
    "electron": "^13.6.1",
    "electron-builder": "^22.13.1",
    "electron-packager": "^14.2.1",
    "npm-watch": "^0.11.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.3",
    "typescript": "^4.3.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
